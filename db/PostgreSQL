-- Enable necessary extensions
CREATE EXTENSION IF NOT EXISTS "uuid-ossp";

-- Create tables
CREATE TABLE users (
  user_id SERIAL PRIMARY KEY,
  username VARCHAR(50) NOT NULL UNIQUE,
  full_name VARCHAR(50),
  phone VARCHAR(10) NOT NULL,
  email VARCHAR(50) NOT NULL UNIQUE,
  role VARCHAR(10),
  password VARCHAR(255) NOT NULL,
  created_at TIMESTAMP NOT NULL DEFAULT NOW(),
  is_active BOOLEAN DEFAULT TRUE,
  avatar_url VARCHAR(255)
);

CREATE TABLE properties (
  property_id SERIAL PRIMARY KEY,
  user_id INTEGER REFERENCES users(user_id),
  property_type VARCHAR(10),
  image_url VARCHAR(255),
  status VARCHAR(20),
  location VARCHAR(255),
  price NUMERIC(10,2),
  title VARCHAR(255),
  description TEXT,
  bedrooms INTEGER,
  bathrooms INTEGER,
  square_ft INTEGER,
  keyword VARCHAR(255) NOT NULL,
  created_at TIMESTAMP NOT NULL DEFAULT NOW(),
  featured BOOLEAN DEFAULT FALSE,
  availability BOOLEAN DEFAULT TRUE
);

CREATE TABLE activities (
  activity_id SERIAL PRIMARY KEY,
  activity_description VARCHAR(255),
  activity_date TIMESTAMP NOT NULL DEFAULT NOW(),
  user_id INTEGER REFERENCES users(user_id)
);

CREATE TABLE issuedproperties (
  issue_id SERIAL PRIMARY KEY,
  property_id INTEGER REFERENCES properties(property_id),
  user_id INTEGER REFERENCES users(user_id),
  date_taken DATE NOT NULL,
  agent_id INTEGER REFERENCES users(user_id)
);

CREATE TABLE payments (
  payment_id SERIAL PRIMARY KEY,
  property_id INTEGER REFERENCES properties(property_id),
  user_id INTEGER REFERENCES users(user_id),
  payment_amount NUMERIC(10,2) NOT NULL,
  phone INTEGER NOT NULL,
  payment_method VARCHAR(50),
  payment_status VARCHAR(50) NOT NULL DEFAULT 'pending',
  payment_date TIMESTAMP NOT NULL DEFAULT NOW()
);

CREATE TABLE search (
  search_id SERIAL PRIMARY KEY,
  user_id INTEGER REFERENCES users(user_id),
  property_id INTEGER REFERENCES properties(property_id),
  property_type VARCHAR(10),
  location VARCHAR(255),
  keyword VARCHAR(255),
  search_date TIMESTAMP NOT NULL DEFAULT NOW()
);

CREATE TABLE testimonials (
  testimonial_id SERIAL PRIMARY KEY,
  username VARCHAR(255),
  user_image_url VARCHAR(255),
  rating NUMERIC(3,2),
  review TEXT,
  created_at TIMESTAMP NOT NULL DEFAULT NOW()
);

